{
    "env": {
      "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
      // add customize rules here as per your project's needs
    },
    "ignorePatterns": ["functions.js", "functions.test.js"]
  }
